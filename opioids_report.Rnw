<<echo=FALSE, results=hide>>=
## fig 1 ##
overdoses <- transform(overdoses, state_code=reorder(state_code, Deaths)) 
pl<-ggplot(overdoses, aes(state_code, Deaths, fill=State,width=.7))+
    scale_y_continuous(limits=c(0,max(overdoses$Deaths)),breaks=seq(0,max(overdoses$Deaths),500))+
  geom_bar(stat="identity")+
  theme(panel.grid.major = element_line(color = "grey"),text = element_text(size=6)
  ,axis.text.x=element_text(angle=70,hjust=1,vjust=1,size=6))+
  labs(y="DEATHS", x="STATE")+

pdf(paste('overdose1', '.pdf', sep=''))
pl
dev.off()
@



<<echo=FALSE, results=tex>>=
cat("\\clearpage",
    "\\begin{figure}[!htbp]",
    "\\begin{center}",
