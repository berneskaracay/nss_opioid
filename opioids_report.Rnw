## fig 2 ##


overdoses <- transform(overdoses, state_code=reorder(state_code, death_percent)) 

pl<-ggplot(overdoses, aes(state_code, death_percent, fill=State,width=.7))+
    scale_y_continuous(limits=c(0,max(overdoses$death_percent)),labels = function(x) paste0(x, "%"))+
  geom_bar(stat="identity")+
  theme(panel.grid.major = element_line(color = "grey"),text = element_text(size=6)
  ,axis.text.x=element_text(angle=70,hjust=1,vjust=1,size=6))+
  labs(y="Deaths percent due to opioid", x="STATE")+

pdf(paste('overdose2', '.pdf', sep=''))
pl
dev.off()
@

